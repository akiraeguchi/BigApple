<%= form_with model:video, local:true do |f| %>
  <div class="form-group">
    <%= f.label :タイトル %>
    <%= f.text_field :title, class: 'form-control video_title' %>
  </div>
  <div class="form-group">
    <%= f.label :コメント %>
    <%= f.text_area :body, class: 'form-control video_body' %>
  </div>

  <div class="form-group">
    <%= f.label :Youtubeリンク %>
    <%= f.text_field :youtube, class: 'form-control video_url' %>
  </div>

  <div class="form-group">
    <%= f.label :撮影日 %>
    <%= f.date_field :video_date, class: 'form-control video_date' %>
  </div>

  <div class="form-group">
    <%= f.submit class: 'btn btn-success' %>
  </div>
<% end %>
